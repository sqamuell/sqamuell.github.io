{
  "name": "website-bun",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "format": "npx @biomejs/biome check --diagnostic-level=error --max-diagnostics=100 --log-kind=compact --apply-unsafe --organize-imports-enabled=true ./app",
    "build": "remix vite:build && cp bun.lockb build/bun.lockb && cp package.json build/package.json && cp server.js build/server.js",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc",
    "transfer": "rsync -avz --exclude 'node_modules' --delete build/ samlosi.com:/var/www/website/",
    "deploy": "bun run build && bun run transfer && ssh samlosi.com -t 'cd /var/www/website && /root/.bun/bin/bun install --production && source ~/.nvm/nvm.sh && /root/.nvm/versions/node/v20.12.2/bin/pm2 reload website && exit'"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@react-navigation/native": "^6.1.17",
    "@react-three/drei": "^9.105.1",
    "@react-three/fiber": "^8.16.1",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "expo": "^50.0.14",
    "gh-pages": "^6.1.1",
    "isbot": "^4.1.0",
    "lucide-react": "^0.378.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.73.6",
    "react-three-fiber": "^6.0.13",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.163.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.3",
    "@remix-run/dev": "^2.8.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}